<div class="card-main">
	<h1>Mes Éxécutions</h1>
	<p>Récapitulatif des derniers ordres</p>
	
	<div class= "container-logs" >
		
		<% @logs.each do |log| %>
			<div class="card-log">

				<h3 class= "card-log-title"> Je suis un titre bien trop long pour être bons</h3>
				<div class= card-log-meta>
					<div class= card-log-meta-network >
						<p>
							<%= icon("fas", "globe-europe") %>
							<%= log.order.network.network_name %>
							
						</p>
					</div>
					<div class= card-log-meta-tag >
						<p>
							<%= icon("fas", "rocket") %>
							<%= log.order.order_tag %>
							
						</p>
					</div>
					<div class= card-log-meta-network>
						<p>
							<%= icon("far", "clock") %>
							<%= log.exectime %>
						</p>
					</div>
					
					
				</div>
				<!-- <%= "#{log.order.network.network_name} - #{log.order.order_tag}" %>  -->
				<p class= "card-log-args"> Paramètres utilisés </p>
				<h4 class= "card-log-results"> Résultats: </h4>
				
				
				<div class="container-logs-messages">
					
					<%log.message.split(";").each do |msg| %>

						<%case prefix_of_log(msg) %>
							<% when "SUCCESS"%>
					<div class="log log-success">
							<% when "WARNING"%>
					<div class = "log log-warning">
							<% when "FAIL"%>
					<div class = "log log-fail">
						<% end %>

							<%= unprefix_this_log msg %>
					</div>
					<% end %>
				</div>

				<p class= "card-log-exectime"> Date d'éxécution juste en bas</p>

			</div>

		<% end %>
	</div>	

</div>